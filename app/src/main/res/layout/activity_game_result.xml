<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/img_game_result_w"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/btn_game_center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="480dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/gmarketsans_ttf_medium"
        android:onClick="onClick"
        android:src="@drawable/center_button"
        android:text="상담센터 "
        android:textColor="@color/black"
        android:textSize="24sp"
        app:backgroundTint="#80FFFFFF"
        app:elevation="12dp"
        app:layout_constraintEnd_toEndOf="@id/img_game_result_w"
        app:layout_constraintHorizontal_bias="0.445"
        app:layout_constraintStart_toStartOf="@id/img_game_result_w"
        app:layout_constraintTop_toTopOf="@id/img_game_result_w" />

    <TextView
        android:id="@+id/m_result"
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="110dp"
        android:fontFamily="@font/gmarketsans_ttf_bold"
        android:gravity="center|center_vertical"
        android:letterSpacing="0.1"
        android:lineSpacingExtra="8sp"
        android:onClick="onClick"
        android:text="결과 보기 click"
        android:textColor="#FF5722"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_game_exit"
        android:layout_width="90dp"
        android:layout_height="48dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:fontFamily="@font/gmarketsans_ttf_medium"
        android:onClick="onClick"
        android:text="메인가기"
        android:textColor="@color/black"
        android:textSize="13sp"
        app:backgroundTint="#99CFCFCF"
        app:layout_constraintEnd_toEndOf="@id/img_game_result_w"
        app:layout_constraintTop_toTopOf="@id/img_game_result_w" />

    <TextView
        android:id="@+id/et_game_warning"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/gmarketsans_ttf_bold"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/m_result" />


</androidx.constraintlayout.widget.ConstraintLayout>