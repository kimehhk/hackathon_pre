<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"><![CDATA[
    android:layout_height="match_parent"
    android:onClick="onClick">

    ]]>

    <ImageView
        android:id="@+id/img_game_result_w"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/button_game_center"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginTop="440dp"
        android:background="#00FFFFFF"
        android:gravity="center"
        android:onClick="onClick"
        android:text="상담센터 연결"
        android:textIsSelectable="false"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@id/img_game_result_w"
        app:layout_constraintStart_toStartOf="@id/img_game_result_w"
        app:layout_constraintTop_toTopOf="@id/img_game_result_w" />

    <TextView
        android:id="@+id/m_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:fontFamily="@font/kopub_dotum_bold"
        android:onClick="onClick"
        android:text="결과 보기 click"
        android:textColor="#FF5722"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_game_exit"
        android:layout_width="120dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:fontFamily="@font/kopub_dotum_bold"
        android:onClick="onClick"
        android:text="메인화면 가기"
        android:textColor="@color/black"
        app:backgroundTint="#80838181"
        app:layout_constraintEnd_toEndOf="@id/img_game_result_w"
        app:layout_constraintTop_toTopOf="@id/img_game_result_w" />


</androidx.constraintlayout.widget.ConstraintLayout>